{
  "mcpProxy": {
    "name": "MCP Lazy Load Proxy - PERFORMANCE EDITION",
    "version": "5.2.0",
    "type": "stdio",
    "hierarchyPath": "c:\\All-Beta-Builds\\lazy-mcp\\testdata\\mcp_hierarchy",
    "options": {
      "lazyLoad": true
    }
  },
  "mcpServers": {
    "playwright": {
      "transportType": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@playwright/mcp"
      ],
      "env": {},
      "options": {
        "lazyLoad": true
      }
    },
    "puppeteer": {
      "transportType": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "env": {},
      "options": {
        "lazyLoad": true
      }
    },
    "filesystem": {
      "transportType": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "c:\\"
      ],
      "env": {},
      "options": {
        "lazyLoad": true
      }
    },
    "sequential-thinking": {
      "transportType": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "env": {},
      "options": {
        "lazyLoad": true
      }
    },
    "github": {
      "transportType": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      },
      "options": {
        "lazyLoad": true
      }
    },
    "github-docker": {
      "transportType": "stdio",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      },
      "options": {
        "lazyLoad": true
      }
    },
    "firecrawl": {
      "transportType": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "firecrawl-mcp"
      ],
      "env": {
        "FIRECRAWL_API_KEY": "${FIRECRAWL_API_KEY}"
      },
      "options": {
        "lazyLoad": true
      }
    },
    "tavily": {
      "options": {
        "lazyLoad": true
      },
      "env": {
        "TAVILY_API_KEY": "${TAVILY_API_KEY}"
      },
      "transportType": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "mcp-remote",
        "https://mcp.tavily.com/mcp/",
        "--header",
        "Authorization:Bearer ${TAVILY_API_KEY}"
      ]
    },
    "desktop-commander": {
      "transportType": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@smithery/cli@latest",
        "run",
        "@wonderwhy-er/desktop-commander",
        "--key",
        "0fca738c-13a2-41c1-8d9a-0c14131ebcb3",
        "--profile",
        "sacred-felidae-XOUu2j"
      ],
      "env": {},
      "options": {
        "lazyLoad": true
      }
    }
  }
}

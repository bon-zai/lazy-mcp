{
  "tools": {
    "merge_pull_request": {
      "description": "Merge a pull request in a GitHub repository.",
      "maps_to": "merge_pull_request",
      "server": "github-docker",
      "inputSchema": {
        "properties": {
          "commit_message": {
            "description": "Extra detail for merge commit",
            "type": "string"
          },
          "commit_title": {
            "description": "Title for merge commit",
            "type": "string"
          },
          "merge_method": {
            "description": "Merge method",
            "enum": [
              "merge",
              "squash",
              "rebase"
            ],
            "type": "string"
          },
          "owner": {
            "description": "Repository owner",
            "type": "string"
          },
          "pullNumber": {
            "description": "Pull request number",
            "type": "number"
          },
          "repo": {
            "description": "Repository name",
            "type": "string"
          }
        },
        "required": [
          "owner",
          "repo",
          "pullNumber"
        ],
        "type": "object"
      }
    }
  }
}

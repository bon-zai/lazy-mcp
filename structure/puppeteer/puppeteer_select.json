{
  "tools": {
    "puppeteer_select": {
      "description": "Select an element on the page with Select tag",
      "maps_to": "puppeteer_select",
      "server": "puppeteer",
      "inputSchema": {
        "properties": {
          "selector": {
            "description": "CSS selector for element to select",
            "type": "string"
          },
          "value": {
            "description": "Value to select",
            "type": "string"
          }
        },
        "required": [
          "selector",
          "value"
        ],
        "type": "object"
      }
    }
  }
}
